            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "e56f7c66-da3d-44df-8298-02f2885b0808"
                }
            }
        },
        "PrivateSubnet3": {
            "Type": "AWS::EC2::Subnet",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "AvailabilityZone": "us-east-1a",
                "CidrBlock": {
                    "Ref": "PrivateSubnet3Cidr"
                },
