        },
        "VPNGateway": {
            "Type": "AWS::EC2::VPNGateway",
            "Properties": {
                "Type": "ipsec.1",
                "Tags": [
                    {
                        "Key": "Environment",
                        "Value": {
                            "Ref": "Environment"
                        }
                    },
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "VPNGatewayName"
                        }
                    }
                ]
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "ffb32df0-2f0a-4632-bb67-f322c4a890fe"
                }
            }
        },
        "VPNGatewayAttachment": {
            "Type": "AWS::EC2::VPCGatewayAttachment",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "VpnGatewayId": {
                    "Ref": "VPNGateway"
                }
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "d2d6862b-323b-4063-9cd8-281e767259bd"
                }
            }
        },
        "CustomerGateway": {
            "Type": "AWS::EC2::CustomerGateway",
            "Properties": {
                "Type": "ipsec.1",
                "BgpAsn": "65000",
                "IpAddress": {
                    "Ref": "VPNAddress"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "CustomerGatewayName"
                        }
                    },
                    {
                        "Key": "VPN",
                        "Value": {
                            "Fn::Join": [
                                "",
                                [
                                    "Gateway to ",
                                    {
                                        "Ref": "VPNAddress"
                                    }
                                ]
                            ]
                        }
                    }
                ]
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "a57e03ef-7230-4e4d-ace0-7cbd19f0a8c6"
                }
            }
        },
        "VPNConnection": {
            "Type": "AWS::EC2::VPNConnection",
            "Properties": {
                "Type": "ipsec.1",
                "StaticRoutesOnly": "true",
                "CustomerGatewayId": {
                    "Ref": "CustomerGateway"
                },
                "VpnGatewayId": {
                    "Ref": "VPNGateway"
                }
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "5d6c364e-8f06-4768-9e76-5d50aa9ac882"
                }
            }
        },
        "VPNConnectionRoute": {
            "Type": "AWS::EC2::VPNConnectionRoute",
            "Properties": {
                "VpnConnectionId": {
                    "Ref": "VPNConnection"
                },
                "DestinationCidrBlock": {
                    "Ref": "OnPremiseCIDR"
                }
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "aa20413e-a7a1-4875-8427-d4d5cc49cb5a"
                }
            }
        },
        "PublicRouteTable": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "PublicRouteTableName"
                        }
                    },
                    {
                        "Key": "Network",
                        "Value": "Connected Subnet Public"
                    }
                ]
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "da1e6b3b-11e9-4725-a061-7f23b8622ca1"
                }
            }
        },
        "PrivateRouteTable": {
            "Type": "AWS::EC2::RouteTable",
            "Properties": {
                "VpcId": {
                    "Ref": "VPC"
                },
                "Tags": [
                    {
                        "Key": "Name",
                        "Value": {
                            "Ref": "PrivateRouteTableName"
                        }
                    },
                    {
                        "Key": "Network",
                        "Value": "Connected Private Public"
                    }
                ]
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "3d653f55-544a-4e0b-8d64-6161f4b4804d"
                }
            }
        },
        "PublicSubnetRouteTableAssociation": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "SubnetId": {
                    "Ref": "PublicSubnet2"
                },
                "RouteTableId": {
                    "Ref": "PublicRouteTable"
                }
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "c4d94701-3e29-4794-a197-0cabb596f0a5"
                }
            }
        },
        "PrivateRoute": {
            "Type": "AWS::EC2::Route",
            "DependsOn": "VPNGatewayAttachment",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PrivateRouteTable"
                },
                "DestinationCidrBlock": {
                    "Ref": "OnPremiseCIDR"
                },
                "GatewayId": {
                    "Ref": "VPNGateway"
                }
            },
            "Metadata": {
                "AWS::CloudFormation::Designer": {
                    "id": "ebc901b8-4693-4216-a163-731da6f4087d"
                }
            }
        },
        "myVPNGatewayRouteProp" : {
          "Type" : "AWS::EC2::VPNGatewayRoutePropagation",
          "Properties" : {
            "RouteTableIds" : [{"Ref" : "PublicRouteTable"}, {"Ref" : "PrivateRouteTable"}],
            "VpnGatewayId" : {"Ref" : "VPNGateway"}
          }
        },
        "EC2SRTAW3WQ": {
            "Type": "AWS::EC2::SubnetRouteTableAssociation",
            "Properties": {
                "RouteTableId": {
                    "Ref": "PublicRouteTable"
                },
                "SubnetId": {
